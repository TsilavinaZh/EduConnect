{
  "expo": {
    "name": "EduConnect",
    "slug": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/OnifraLogo.jpg",
    "scheme": "EduConnect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/OnifraLogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tsiory518c.apkonifra"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/OnifraLogo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/OnifraLogo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "993a9b79-a441-4184-911f-738b63414f92"
      }
    },
    "owner": "tsiory518c"
  }
}
